{
  "name": "@one-ecosystem/engine",
  "version": "1.0.0",
  "description": "ONE Engine - Unified backend API and SDK for the ONE ecosystem",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4000",
    "build": "next build",
    "start": "next start -p 4000",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:generate": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/database/types.ts",
    "db:migrate": "supabase db push",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@supabase/ssr": "^0.5.0",
    "@supabase/supabase-js": "^2.45.0",
    "@types/jsonwebtoken": "^9.0.10",
    "ccxt": "^4.3.0",
    "cron": "^3.1.0",
    "date-fns": "^3.6.0",
    "ioredis": "^5.4.0",
    "jose": "^5.6.0",
    "jsonwebtoken": "^9.0.3",
    "next": "^14.2.0",
    "openai": "^4.55.0",
    "pino": "^9.3.0",
    "pino-pretty": "^11.2.0",
    "qrcode": "^1.5.4",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "thirdweb": "^5.90.0",
    "uuid": "^10.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "supabase": "^1.187.0",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
